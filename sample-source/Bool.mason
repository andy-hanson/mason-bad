use .types for Bool

True.
	`true`

False.
	`false`

not. |a:Bool
	`!a`

and. |a:Bool b:Act
	`a && b()`

or. |a:Bool b:Act
	`a || b()`

nand. |a:Bool b:Act
	not (and a b)

nor. |a:Bool b:Act
	not (or a b)

truthy?. |:Bool a
	doc
		Whether JavaScript will consider `a` truthy.

	`!!a`

xor. |a:Bool b:Bool
	`a !== b`

