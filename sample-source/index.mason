use .Bool for True False and or not nand nor xor
use .arithmetic for + - * / < > abs
use .control for ? if unless while times
use .Vec2
use .method for method implement



size = method
	name. "sizee"

implement size Number |n
	abs n

implement size Array |arr
	arr.length


log! (size 3)
log! (size
	. 1
	. 2
)

