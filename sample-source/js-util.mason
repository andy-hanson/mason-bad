use .alias for Act Fun

identical?. |a b
	`a === b`

new. |constructor:Function
	boundConstructor = constructor.bind constructor (arguments.slice 1)
	`new boundConstructor`

send. |object name:String
	(property object name).apply object (arguments.slice 2)

property. |object name:String
	`object[name]`

\ True unless a is null or undefined.
exists?. |a
	`a != null`

is-a?. |a type:Fun
	`Object(a) instanceof type`

truthy?. |:Bool a
	`!!a`

do. |todo:Act
	`todo()`

